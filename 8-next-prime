public Integer nextPrime(Integer num) {
    //code here

    Integer n = num + 1;

    while (!isPrime(n)) {
        n++;
    }
    return n;
}
    public Boolean isPrime(Integer n) {
        for (Integer i = 1; i <= n; i++) {
            if (Math.mod(n, i) == 0) {
                return false;
            } else {
                return true;
            }
        }
    }

